{
    "manifest_version": 2,
    "name": "grab",
    "version": "1.0",

    "description": "grab videos from streaming sites.",
    "icons": {
        "48": "icons/beasts-48.png"
    },

    "permissions": [
        "activeTab", "downloads"
    ],

    "browser_action": {
        "default_icon": "icons/beasts-32.png",
        "default_title": "grab video",
        "default_popup": "popup/popup_grab.html"
    }
}